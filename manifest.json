{
  "manifest_version": 3,
  "name": "Discord Token Login",
  "version": "1.0",
  "description": "Inicia sesi√≥n en Discord usando tu token.",
  "permissions": ["storage", "scripting", "tabs"],
    "content_scripts": [
      {
        "matches": ["https://discord.com/*"],
        "js": ["inject.js"],
        "run_at": "document_start"
      }
    ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
